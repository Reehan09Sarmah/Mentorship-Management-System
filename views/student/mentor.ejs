<% layout('layouts/SMBoilerplate') %>

    <div class="col-7 col-md-8 m-4">
        <div class="row">
            <div class="col-sm-6 ">
                <h2>Mentor</h2>
                <hr>
                <div class="card shadow-sm p-2 mb-3 bg-body rounded">
                    <img class="card-img-top"
                        src="data:image/jpeg;base64,"
                        alt="Card image cap">
                    <div class="card-body">
                        <div class="border p-3 rounded">
                            <h5 class="card-title">
                                <%= mentor.name %>
                            </h5>
                            <ul class="card-text list-unstyled">
                                <li>Department : <em class="lead">
                                        <%= mentor.department %>
                                    </em></li>

                                <li> Email: <em class="lead">
                                        <%= mentor.email %>
                                    </em></li>

                            </ul>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-sm-6">
                <% if(mentor.mentees.length) {%>
                    <h2>Current Mentees</h2>
                    <hr>
                    <div class="border shadow-sm p-3 bg-body rounded">
                        <% for(let mentee of mentor.mentees) {%>
                            <div class="card shadow-sm p-2 mb-3 bg-body rounded">
                                <div class="card-body">
                                    <h5 class="card-title">
                                        <%= mentee.name %>
                                    </h5>
                                    <details class="mb-3">
                                        <b> Program: </b>
                                        <%=mentee.program%> <br>
                                            <b>Roll Number:</b>
                                            <%=mentee.rollno%>
                                                <br>
                                                <b>Semester:</b>
                                                <%=mentee.currentSemester%>
                                    </details>

                                </div>
                            </div>

                            <% } %>
                    </div>
            </div>
            <%}%>
        </div>

    </div>
